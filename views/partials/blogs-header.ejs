<body onload="myFunc()">
  <!-- <body> -->

  <div class="loader_bg">
    <div class="loader"></div>
  </div>

  <!-- =========== Navbar ===========-->

  <nav class="navbar navbar-expand-lg bg-info navbar-light" id="navig">

    <a class="navbar-brand font-weight-bold text-uppercase" style="color: white;" href="#">Tauheed Shahid
    </a>

    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNavAltMarkup"
      aria-controls="navbarNavAltMarkup" aria-expanded="false" aria-label="Toggle navigation">
      <span class="navbar-toggler-icon"></span>
    </button>

    <div class="collapse navbar-collapse" id="navbarNavAltMarkup">

      <ul class="navbar-nav text-uppercase ml-auto">


        <% if(user!= undefined && (user.type) == "admin") {%>

        <li class="nav-item">
          <a class="nav-link" href="/compose">Compose</a>
        </li>

        <% } %>

        <li class="nav-item">
          <a class="nav-link" href="/">Personal</a>
        </li>

        <li class="nav-item">
          <a class="nav-link" href="/gfg">GeeksforGeeks</a>
        </li>
     

        <% if(!user) { %>

        <li class="nav-item">
          <a class="nav-link" href="/login">Login</a>
        </li>

        <% } else { %>

        <li class="nav-item dropdown">

          <a class="nav-link dropdown-toggle" id="navbarDropdown"  role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
            Welcome <%= user.username %>
          </a>

          <div class="dropdown-menu dropdown_menu" aria-labelledby="navbarDropdown">
            <a class="dropdown-item" href="/userAccount">Account</a>
            <div class="dropdown-divider"></div>
            <a class="dropdown-item" href="/logout">Logout</a>
          </div>

        </li>

        <% } %>
      </ul>

    </div>
  </nav>